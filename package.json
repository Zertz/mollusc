{
  "name": "mollusc",
  "description": "slugifies even utf-8 chars!",
  "version": "0.9.2",
  "homepage": "https://github.com/Zertz/mollusc",
  "author": "Zertz (https://github.com/Zertz)",
  "repository": {
    "type": "git",
    "url": "git://github.com/Zertz/mollusc.git"
  },
  "main": "slug.js",
  "engines": {
    "node": ">= 0.4.x"
  },
  "keywords": [
    "slugify",
    "slug",
    "string",
    "utf8",
    "utf-8",
    "unicode",
    "url"
  ],
  "scripts": {
    "lint": "standard slug.js",
    "pretest": "npm run lint",
    "test": "./node_modules/.bin/mocha ./test/*.test.* --require should --reporter spec --colors --compilers coffee:coffee-script/register"
  },
  "dependencies": {
    "unicode-json": "~1.0.0"
  },
  "devDependencies": {
    "coffee-script": "^1.7.1",
    "mocha": "^1.17.1",
    "should": "^3.1.2",
    "standard": "^6.0.8"
  },
  "bin": {
    "slug": "bin/slug.js"
  },
  "license": "MIT",
  "standard": {
    "globals": [
      "define"
    ]
  }
}
