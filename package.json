{
  "name": "mollusc",
  "description": "slugifies even utf-8 chars!",
  "version": "1.2.0",
  "homepage": "https://github.com/Zertz/mollusc",
  "author": "Zertz (https://github.com/Zertz)",
  "repository": {
    "type": "git",
    "url": "git://github.com/Zertz/mollusc.git"
  },
  "main": "./src",
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "slugify",
    "slug",
    "string",
    "utf8",
    "utf-8",
    "unicode",
    "url"
  ],
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "test": "mocha -R spec ./test",
    "test-coverage": "nyc npm run test",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "unicode": "^10.0.0"
  },
  "devDependencies": {
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "should": "^3.1.2",
    "standard": "^10.0.3"
  },
  "license": "MIT",
  "standard": {
    "globals": [
      "define",
      "describe",
      "it"
    ]
  },
  "files": [
    "src"
  ]
}
